{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
